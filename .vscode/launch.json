{
    "version": "0.2.0",
    "configurations": [
    {
        "name": "Run Backend",
        "type": "node",
        "request": "launch",

        "cwd": "${workspaceFolder}/project",
        "runtimeArgs": [
            "-r",
            "ts-node/register"
        ],
        "args": [
            "backend/main.ts"
        ],
        "env": {
          "TS_NODE_PROJECT": "backend/tsconfig.json"
        },
        "outputCapture": "std"
    }
    ]
}
